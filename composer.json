{
    "name": "phpgenesis/common",
    "description": "Shared Functionality for PHPGenesis",
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "php": "^8.3|^8.4|^8.5",
        "composer-runtime-api": "^2.0",
        "composer/composer": "^2.0",
        "encoredigitalgroup/stdlib": "^2.0",
        "illuminate/collections": "^10|^11|^12|^13",
        "illuminate/database": "^10|^11|^12|^13",
        "illuminate/support": "^10|^11|^12|^13"
    },
    "require-dev": {
        "laravel/framework": "^10|^11|^12|^13"
    },
    "autoload": {
        "psr-4": {
            "PHPGenesis\\Common\\": "src/"
        },
        "files": [
            "phpgenesis_helpers.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "PHPGenesis\\Common\\Composer\\Scripts::postAutoloadDump"
        ]
    },
    "minimum-stability": "stable"
}
