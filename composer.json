{
    "name": "phpgenesis/common",
    "description": "Shared Functionality for PHPGenesis",
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.3",
        "composer-runtime-api": "^2.0",
        "composer/composer": "^2.0",
        "encoredigitalgroup/stdlib": "^1.3",
        "illuminate/collections": "^10|^11",
        "illuminate/database": "^10|^11",
        "illuminate/support": "^10|^11"
    },
    "scripts": {
        "post-autoload-dump": [
            "PHPGenesis\\Common\\Composer\\Scripts::postAutoloadDump"
        ]
    },
    "autoload": {
        "psr-4": {
            "PHPGenesis\\Common\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php",
            "phpgenesis_helpers.php"
        ]
    },
    "minimum-stability": "stable"
}
